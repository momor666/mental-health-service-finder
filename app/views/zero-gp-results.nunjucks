{% extends 'layout.nunjucks' %}

{% block header %}
<h1>Sorry, we couldn't find any GP surgeries matching '{{ cleanQuery }}'</h1>
{% endblock %}

{% block content %}
<p>Please try:</p>
  <ul>
    <li>searching again using different search terms</li>
    <li>checking your spelling</li>
  </ul>
{% endblock %}

{% block back %}
{% set href = siteRoot + "/search?query=" + cleanQuery %}
{{ linkBack(href) }}
{% endblock %}
